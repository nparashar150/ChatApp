  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "electron-icon-maker": "^0.0.5"
  },
  "dependencies": {
    "concurrently": "^6.5.1",
    "electron-is-dev": "^2.0.0"
